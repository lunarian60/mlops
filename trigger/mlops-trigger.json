    "type": "endpoint",
    "region": "ap-northeast-2",
    "resourcePrefix": "xgboost",
    "roleArn": "arn:aws:iam::145081548823:role/service-role/AmazonSageMaker-ExecutionRole-20210831T134631",
    "sageMakerContainerArn": "366743142698.dkr.ecr.ap-northeast-2.amazonaws.com/sagemaker-xgboost:1.3-1",
